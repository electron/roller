{
  "name": "roller",
  "version": "1.0.0",
  "description": "automate the submodule update process",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --check \"src/**/*.ts\"",
    "prettier:write": "prettier --write \"src/**/*.ts\"",
    "prestart": "tsc",
    "prepare": "tsc",
    "roll-chromium": "DEBUG=roller* node lib/chromium-cron.js",
    "roll-node": "DEBUG=roller* node lib/node-cron.js",
    "start": "echo \"Starting roller bot.\" && exit 0",
    "test": "jest --config=jest.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/roller.git"
  },
  "author": "Shelley Vohr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electron/roller/issues"
  },
  "homepage": "https://github.com/electron/roller#readme",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3"
  },
  "dependencies": {
    "@octokit/rest": "^16.24.3",
    "@slack/client": "^4.4.0",
    "@types/semver": "^6.0.1",
    "body-parser": "^1.18.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-github-webhook": "^1.0.6",
    "prettier": "^1.19.1",
    "semver": "^6.2.0",
    "typescript": "^3.0.1"
  }
}
